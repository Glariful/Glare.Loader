<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <!-- MainWindow TopBar -->
    <system:String x:Key="Settings">Settings</system:String>
    
    <!-- MainWindow Tabs -->
    <system:String x:Key="News">News</system:String>
    <system:String x:Key="Loader">Loader</system:String>

    <!-- MainWindow Buttons -->
    <system:String x:Key="AcceptTosButton">I Accept</system:String>
    <system:String x:Key="DeclineTosButton">I Decline</system:String>
    
    <!-- Settings -->

    <!-- Errors and other stuff -->
    <system:String x:Key="WrongAuth">Wrong password or username, register at {0}</system:String>
    <system:String x:Key="AuthEmpty">Password or username is empty</system:String>
    <system:String x:Key="FailedToLogin">Failed to login: {0}</system:String>
    <system:String x:Key="Login">Login</system:String>
    <system:String x:Key="LoginError">Failed to sign in as {0}: {1}</system:String>

</ResourceDictionary>